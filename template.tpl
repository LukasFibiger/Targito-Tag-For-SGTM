//Setup constants
const JSON = require('JSON');
const logToConsole = require('logToConsole');
const sendHttpRequest = require('sendHttpRequest');

//If debug_mode On, write complete JSON to Console
if (data.debugMode == true) {
  logToConsole(JSON.stringify({
    'Point': 'Targito - debug - input data',
    'Body': data
  }));
}

//Screen resolution prepare. Split screen resolution to two extra parameters
var screen_resolution = data.resolution.split('x');
var screen_width = screen_resolution[0];
var screen_height = screen_resolution[1];

//Setup basic array for POST request to Targito
var post_data = {
  "account_id": data.account_id,
  "contact_id": data.contact_id,
  "mailing_id": data.mailing_id,
  "mailing_date": data.mailing_date,
  "origin": data.origin,
  "request_info": {
    "t": data.timezone_offset,
    "r": data.referrer,
    "a": data.user_agent,
    "l": data.language,
    "w": screen_width,
    "h": screen_height
  }
};


//Convert products from ecommerce format to Targito format. If products exists. 
if (typeof data.products != 'undefined') {
  //convert JSON string to object, if necessary
  if (typeof data.products == 'string') {
    data.products = JSON.parse(data.products);
  }
  var targito_products = [] ;
  data.products.forEach(function(produkt, index) {
    targito_products[index] = {
      'item_id': produkt.item_id,
      'item_price': produkt.price,
      "item_price_vat": produkt.priceWithTax,
      'item_quantity': produkt.quantity
    };
  });
}

//Setup Endpoint URL
var url_api = 'https://t.targito.tescoma.pl/s2s/'+data.endpoint;

//Setup values for endpoints
switch(data.endpoint) {
  case 'pageview':
    post_data.page_url = data.page_url;
    break;
  case 'event':
    post_data.name = data.event_name;
    post_data.attributes = {
      'id': data.event_id,
    };    
    break;
  case 'property':
    post_data.name = data.property_name;
    post_data.attributes = {
      'url': data.property_cart_url,
      'total': data.total_value,
      'total_vat': data.total_value_vat,
      'products': targito_products
    };    
    break;
  case 'order':
    post_data.order_id = data.order_id;
    post_data.total = data.total_value;
    post_data.total_vat = data.total_value_vat;
    post_data.products = targito_products;
    post_data.attributes = {
      'voucher_code': data.voucher_code
    };    
    //Add custom parameters for order, if it is exists
    if (typeof data.order_param != 'undefined') {
      data.order_param.forEach(function(parameter) {
        post_data.attributes[parameter.param_name] = parameter.param_value ;
      });
    }
    
    
    break;
}

//If debug_mode On, write complete JSON to Console
if (data.debugMode == true) {
  logToConsole(JSON.stringify({
    'Point': 'Targito - debug - output data',
    'Body': post_data
  }));
}

//Send data to targito
sendHttpRequest(url_api, (statusCode, headers, body) => {
  if (statusCode >= 200 && statusCode < 300) {
    if (data.debugMode == true) {
      logToConsole(JSON.stringify({
        'Point': 'Targito - Success',
        'ResponseStatusCode': statusCode,
        'ResponseHeaders': headers,
        'ResponseBody': body,
        'PostData': post_data,
      }));
    }
    //OK, Targito response 20x code
    data.gtmOnSuccess();
  } else {
    if (data.debugMode == true) {
      logToConsole(JSON.stringify({
        'Point': 'Targito - Failure',
        'ResponseStatusCode': statusCode,
        'ResponseHeaders': headers,
        'ResponseBody': body,
        'PostData': post_data,
      }));
    }
    //Error, Targito response not 20x code
    data.gtmOnFailure();
  }},
  {
    headers: {
      'content-type': 'application/json',
      'Authorization': 'Bearer '+data.api_key
    },
    method: 'POST', 
    timeout: 2000
   },
   JSON.stringify(post_data)
);


